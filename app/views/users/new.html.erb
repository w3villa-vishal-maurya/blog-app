<%= form_with model: @user, url: users_path, multipart: true, method: :post  do |form| %>
  

  <div class="mb-3">
    <br/>
    <%= form.label "Username"%>
    <%= form.text_field :username, class: "form-control", placeholder: "username"%>
    <br/>
  </div>
  <div class="mb-3">
    <%= form.label "Email"%>
    <%= form.text_field :email, class: "form-control", placeholder: "email"%>
  </div><br/>
  <div class="mb-3">
    <%= form.label "DOB"%>
    <%= form.date_select :DOB, class: "form-control", placeholder: ""%>
  </div>
  <div class="mb-3">
    <%= form.label "password"%>
    <%= form.password_field :password, class: "form-control", placeholder: "password"%>
  </div><br/>
 
 
  <div class="mb-3">
    <%= form.submit "Register", class: "btn btn-primary"%>
  </div>
<%end%>